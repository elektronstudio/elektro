{
  "name": "elektronstudio",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/elektronstudio.umd.js",
  "module": "./dist/elektronstudio.es.js",
  "exports": {
    ".": {
      "import": "./dist/elektronstudio.es.js",
      "require": "./dist/elektronstudio.umd.js"
    }
  },
  "type": "module",
  "typings": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite -c vite.config.app.ts",
    "build": "vite build -c vite.config.app.ts && vite build -c vite.config.lib.ts && vue-tsc --declaration --emitDeclarationOnly",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^7.1.1",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@vitejs/plugin-vue": "^1.10.0",
    "vite": "^2.6.14",
    "vue-tsc": "^0.29.6"
  }
}
