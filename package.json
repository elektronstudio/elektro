{
  "name": "elektronstudio",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/elektronstudio.umd.js",
  "module": "./dist/elektronstudio.es.js",
  "exports": {
    ".": {
      "import": "./dist/elektronstudio.es.js",
      "require": "./dist/elektronstudio.umd.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite -c vite.config.app.ts",
    "build": "vue-tsc --noEmit && vite build -c vite.config.app.ts && vite build -c vite.config.lib.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^7.1.1",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@vitejs/plugin-vue": "^1.9.2",
    "vue-tsc": "^0.3.0"
  }
}
