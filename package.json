{
  "name": "elektro",
  "version": "0.0.1",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elektronstudio/elektro.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/elektronstudio.umd.js",
  "module": "./dist/elektronstudio.es.js",
  "exports": {
    ".": {
      "import": "./dist/elektronstudio.es.js",
      "require": "./dist/elektronstudio.umd.js"
    }
  },
  "type": "module",
  "typings": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite -c vite.config.app.ts",
    "build": "vite build -c vite.config.app.ts && vite build -c vite.config.lib.ts --emptyOutDir=false && vue-tsc --declaration --emitDeclarationOnly",
    "serve": "vite preview",
    "check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "tailwindcss": "^3.0.11",
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@vitejs/plugin-vue": "^2.0.1",
    "vite": "^2.7.10",
    "vite-plugin-pages": "^0.19.9",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.30.2"
  }
}
